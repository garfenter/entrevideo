<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="
      http://java.sun.com/xml/ns/javaee
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">   
    <managed-bean>
        <managed-bean-name>sessionUI</managed-bean-name>
        <managed-bean-class>gt.com.entrevideo.web.backend.session.SessionUI</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>habilidadesUI</managed-bean-name>
        <managed-bean-class>gt.com.entrevideo.web.backend.cv.habilidades.HabilidadesUI</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>datosGeneralesUI</managed-bean-name>
        <managed-bean-class>gt.com.entrevideo.web.backend.cv.general.DatosGeneralesUI</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>autocompleteUI</managed-bean-name>
        <managed-bean-class>gt.com.entrevideo.component.autocomplete.AutocompleteBeanUI</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>videoUI</managed-bean-name>
        <managed-bean-class>gt.com.entrevideo.web.backend.cv.video.VideoUI</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>blobUploadService</managed-bean-name>
        <managed-bean-class>gt.com.entrevideo.component.fileupload.BlobUploadService</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>   
    <component>
        <component-class>gt.com.entrevideo.component.autocomplete.AutoComplete</component-class>
        <component-type>AutoComplete</component-type> 
              
    </component>
    <render-kit>               
        <renderer>
            <renderer-class>gt.com.entrevideo.component.autocomplete.AutoCompleteRenderer</renderer-class>
            <renderer-type>AutoCompleteRenderer</renderer-type>
            <component-family>gt.com.entrevideo.component</component-family>
        </renderer>
    </render-kit>        
</faces-config>