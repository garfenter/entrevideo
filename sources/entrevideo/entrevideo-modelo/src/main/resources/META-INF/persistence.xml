<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="transactions-optional" transaction-type="RESOURCE_LOCAL">
    <properties>
      <properties>
        <property name="javax.persistence.jdbc.driver" value="com.google.appengine.api.rdbms.AppEngineDriver"/>
        <property name="javax.persistence.jdbc.url" value="jdbc:google:rdbms://entrevideo-db:entrevideodb/entrevideodb"/>
        <property name="javax.persistence.jdbc.user" value="entrevideo_srv"/>
        <property name="javax.persistence.jdbc.password" value="entrevideopass2013"/>
        <!--property name="javax.persistence.jdbc.user" value="root"/-->
        <property name="eclipselink.target-database" value="MySQL"/>
        <property name="eclipselink.platform.class.name" value="org.eclipse.persistence.platform.database.MySQLPlatform"/>
      </properties>
    </properties>
  </persistence-unit>
</persistence>
